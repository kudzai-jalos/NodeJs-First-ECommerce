<%- include('../partials/head.ejs') %>
</head>
<%- include('../partials/navigation.ejs') %>
<h2>Orders</h2>
<ul>
  <% orders.forEach((order)=>{ %>
    <li>
      <h3>Order #<%= order._id%></h3>
      <ul>
        <% order.products.forEach((product)=>{ %>
          <li>
            <h4><%= product.title %> (<%= product.quantity %>)</h4>
          </li>
        <% }) %>
      </ul>
    </li>
  <% }) %>
</ul>
<%- include('../partials/foot.ejs')%>
